{
	"manifest_version": 2,
	"name": "follow",
	"version": "0.0.1",
	"description": "收藏夹",
	"icons":
	{
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"permissions":
	[
        "declarativeContent",
		"tabs",
		"http://*/*", 
		"https://*/*"
    ],
	"background":
	{
		"scripts": ["js/background.js"]
    },
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_title": "follow"
	},
	"default_locale": "zh_CN"
}